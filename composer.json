{
    "name": "turndale/paystack",
    "description": "A Laravel Package for Paystack payment integration for laravel 11 and above",
    "keywords": [
        "paystack laravel package",
        "africa-payment paystack",
        "ghana-payment-laravel",
        "paystack-nigeria-laravel",
        "open-source paystack",
        "contributions paystack",
        "discussions",
        "issue-tracking"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://paystack.stephenasare.dev/",
    "authors": [
        {
            "name": "Stephen Asare",
            "email": "me@stephenasare.dev",
            "homepage": "https://paystack.stephenasare.dev/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.8",
        "phpunit/phpunit": "^12.5"
    },
    "autoload": {
        "psr-4": {
            "Turndale\\Paystack\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Turndale\\Paystack\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Turndale\\Paystack\\PaystackServiceProvider"
            ],
            "aliases": {
                "Paystack": "Turndale\\Paystack\\Facades\\Paystack"
            }
        },
        "branch-alias": {
            "dev-1.x": "1.1-dev"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": false
        }
    }
}