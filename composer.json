{
    "name": "hellofromsteve/paystack",
    "description": "A Laravel Package for Paystack payment integration for laravel 11 and above",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Stephen Asare",
            "email": "stephen@solentik.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.8",
        "phpunit/phpunit": "^12.5"
    },
    "autoload": {
        "psr-4": {
            "HelloFromSteve\\Paystack\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "HelloFromSteve\\Paystack\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HelloFromSteve\\Paystack\\PaystackServiceProvider"
            ],
            "aliases": {
                "Paystack": "HelloFromSteve\\Paystack\\Facades\\Paystack"
            }
        },
        "branch-alias": {
            "dev-main": "1.1-dev"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": false
        }
    }
}
